@model Car_Rent.Models.Category

@{
    ViewData["Title"] = "Category Details";
    Layout = "~/Views/Shared/AdminLayout/_AdminLayout.cshtml";
}

<h2>Category Details</h2>
<hr />

<dl class="row">
